<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zpt.shop.main.mapper.AdminUserMsgMapper">

	<!-- ********** ResultMap ********** -->
	
	<insert id="insertAdminUserMsg" parameterType="adminUserMsg" useGeneratedKeys="true"
		keyProperty="id">
		insert into t_adminuser_msg (username,time)
		values (#{username},#{time})
	</insert>

</mapper>
